<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0" xmlns:xlink="http://www.w3.org/1999/xlink" name="NCEP Model Data" version="1.0.1">
  <service name="latest" serviceType="Resolver" base="" />
  <service name="HTTPServer" serviceType="HTTPServer" base="/thredds/fileServer/" />
  <service name="all" serviceType="Compound" base="">
    <service name="ncdods" serviceType="OPENDAP" base="/thredds/dodsC/" />
    <service name="wcs" serviceType="WCS" base="/thredds/wcs/" />
    <service name="ncServer" serviceType="NetcdfServer" base="/thredds/ncServer/" />
  </service>
  <dataset name="Unidata Real-time Steered Regional Model">
    <metadata inherited="true">
      <serviceName>all</serviceName>
      <authority>edu.ucar.unidata</authority>
      <dataType>Grid</dataType>
      <dataFormat>GRIB-1</dataFormat>
      <documentation type="summary">Steered WRF Model, using accumulated precipitation forecast to choose 2 regions of interest. The model runs every 6 hours, with hourly forecast out to 30 hours.</documentation>
      <documentation type="rights">Freely available</documentation>
      <documentation xlink:href="http://www.unidata.ucar.edu/software/gempak/rtmodel/" xlink:title="Documentation" />
      <creator>
        <name vocabulary="DIF">UCAR/UNIDATA</name>
        <contact url="http://www.unidata.ucar.edu/" email="support@unidata.ucar.edu" />
      </creator>
      <publisher>
        <name vocabulary="DIF">UCAR/UNIDATA</name>
        <contact url="http://www.unidata.ucar.edu/" email="support@unidata.ucar.edu" />
      </publisher>
      <timeCoverage>
        <end>present</end>
        <duration>7 days</duration>
      </timeCoverage>
    </metadata>
    <dataset name="Region One (aggregation)" ID="model/Unidata/SteeredWRF/RegionOne" urlPath="model/Unidata/SteeredWRF/RegionOne.dataset">
      <netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
        <aggregation dimName="run" type="forecastModelRunSingleCollection" timeUnitsChange="true">
          <scanFmrc location="/data/ldm/pub/rtmodel/" regExp=".*_nmm\.GrbF[0-9]{5}$" runDateMatcher="yyMMddHH#_nmm.GrbF#" forecastOffsetMatcher="#_nmm.GrbF#HHH" subdirs="true" />
        </aggregation>
      </netcdf>
    </dataset>
    <datasetScan name="Individual Files" ID="model/Unidata/SteeredWRF" path="model/Unidata/SteeredWRF" location="/data/ldm/pub/rtmodel/">
      <metadata inherited="true">
        <serviceName>HTTPServer</serviceName>
      </metadata>
      <addID />
      <sort>
        <lexigraphicByName increasing="false" />
      </sort>
      <addDatasetSize />
    </datasetScan>
  </dataset>
</catalog>

